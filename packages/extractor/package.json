{
	"name": "@svelstack/translation-extractor",
	"version": "0.9.0",
	"scripts": {
		"build": "tsup && npm run prepack",
		"prepack": "publint",
		"check": "tsc --noEmit",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run"
	},
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"bin": "./dist/index.cjs",
	"exports": {
		"require": "./dist/index.cjs",
		"import": "./dist/index.js"
	},
	"dependencies": {
		"typescript": "^5.7.3",
		"yaml": "^2.7.0"
	},
	"files": [
		"dist",
		"*.d.ts",
		"README.md"
	],
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/node": "^22.10.7",
		"commander": "^13.0.0",
		"publint": "^0.3.2",
		"tsup": "^8.3.5",
		"vitest": "^2.1.8"
	}
}
